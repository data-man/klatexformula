<?xml version="1.0"?>
<klfuserscript-info klf-compat-version="4">
  <exe-script>runpreviewlatex.py</exe-script>
  <can-provide-default-settings>true</can-provide-default-settings>
  <category>klf-backend-engine</category>
  <name>Use {preview} latex package</name>
  <author>Philippe Faist, philippe.faist@bluewin.ch</author>
  <version>1.0</version>
  <license>GPL v2+</license>
  <settings-form-ui>runpreviewlatex_config.ui</settings-form-ui>
  <klf-backend-engine>
    <spits-out>png pdf</spits-out>
    <skip-formats selector="all-except">
      png pdf
    </skip-formats>
    <disable-inputs selector="ALL EXCEPT">
      FG_COLOR MATHMODE PREAMBLE FONT FONTSIZE
      DPI VECTORSCALE MARGINS
    </disable-inputs>
    <input-form-ui>runpreviewlatex_params.ui</input-form-ui>
  </klf-backend-engine>
</klfuserscript-info>
