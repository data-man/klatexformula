<!DOCTYPE UI><UI version="3.3" stdsetdef="1">
<class>KLFHistoryBrowserUI</class>
<comment>KLatexFormula History Browser</comment>
<author>Philippe Faist &lt;philippe.faist@bluewin.ch&gt;</author>
<widget class="QDialog">
    <property name="name">
        <cstring>KLFHistoryBrowserUI</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>791</width>
            <height>464</height>
        </rect>
    </property>
    <property name="caption">
        <string>KLatexFormula History Browser</string>
    </property>
    <property name="sizeGripEnabled">
        <bool>false</bool>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>5</number>
        </property>
        <property name="spacing">
            <number>3</number>
        </property>
        <widget class="QFrame">
            <property name="name">
                <cstring>frmExtra</cstring>
            </property>
            <property name="frameShape">
                <enum>Box</enum>
            </property>
            <property name="frameShadow">
                <enum>Sunken</enum>
            </property>
            <property name="lineWidth">
                <number>1</number>
            </property>
            <hbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>6</number>
                </property>
                <property name="spacing">
                    <number>3</number>
                </property>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>lblSearch</cstring>
                    </property>
                    <property name="minimumSize">
                        <size>
                            <width>100</width>
                            <height>0</height>
                        </size>
                    </property>
                    <property name="text">
                        <string>Search:</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Incremental search within history items. Find next with F3 key.</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>If you insert keywords as LaTeX comments in your formulas ('&lt;tt&gt;%keyword&lt;/tt&gt;&lt;i&gt;[newline]&lt;/i&gt;') then you can search for those formulas here.</string>
                    </property>
                </widget>
                <widget class="QToolButton">
                    <property name="name">
                        <cstring>btnSearchClear</cstring>
                    </property>
                    <property name="text">
                        <string>Clr&gt;</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Clear Search bar</string>
                    </property>
                </widget>
                <widget class="KLineEdit">
                    <property name="name">
                        <cstring>lneSearch</cstring>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Incremental search within history items. Find next with F3 key.</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>If you insert keywords as LaTeX comments in your formulas ('&lt;tt&gt;%keyword&lt;/tt&gt;&lt;i&gt;[newline]&lt;/i&gt;') then you can search for those formulas here.</string>
                    </property>
                </widget>
                <widget class="QToolButton">
                    <property name="name">
                        <cstring>btnFindNext</cstring>
                    </property>
                    <property name="text">
                        <string>Find Next</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Find Next [F3]</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Find Next occurrence of search string</string>
                    </property>
                </widget>
            </hbox>
        </widget>
        <widget class="QListView">
            <column>
                <property name="text">
                    <string>Preview</string>
                </property>
                <property name="clickable">
                    <bool>true</bool>
                </property>
                <property name="resizable">
                    <bool>true</bool>
                </property>
            </column>
            <column>
                <property name="text">
                    <string>LaTeX formula</string>
                </property>
                <property name="clickable">
                    <bool>true</bool>
                </property>
                <property name="resizable">
                    <bool>true</bool>
                </property>
            </column>
            <property name="name">
                <cstring>lstHistory</cstring>
            </property>
            <property name="selectionMode">
                <enum>Extended</enum>
            </property>
            <property name="allColumnsShowFocus">
                <bool>true</bool>
            </property>
            <property name="showSortIndicator">
                <bool>true</bool>
            </property>
        </widget>
        <widget class="QLayoutWidget">
            <property name="name">
                <cstring>layout2</cstring>
            </property>
            <hbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>btnRestore</cstring>
                    </property>
                    <property name="enabled">
                        <bool>false</bool>
                    </property>
                    <property name="sizePolicy">
                        <sizepolicy>
                            <hsizetype>1</hsizetype>
                            <vsizetype>0</vsizetype>
                            <horstretch>1</horstretch>
                            <verstretch>0</verstretch>
                        </sizepolicy>
                    </property>
                    <property name="text">
                        <string>Restore Form&amp;ula</string>
                    </property>
                    <property name="accel">
                        <string>Alt+U</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Restore current selected formula to KLatexFormula editor, with style</string>
                    </property>
                </widget>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>btnDelete</cstring>
                    </property>
                    <property name="enabled">
                        <bool>false</bool>
                    </property>
                    <property name="sizePolicy">
                        <sizepolicy>
                            <hsizetype>1</hsizetype>
                            <vsizetype>0</vsizetype>
                            <horstretch>1</horstretch>
                            <verstretch>0</verstretch>
                        </sizepolicy>
                    </property>
                    <property name="text">
                        <string>Delete item(s)</string>
                    </property>
                    <property name="accel">
                        <string></string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Delete de selected history items</string>
                    </property>
                </widget>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>btnImport</cstring>
                    </property>
                    <property name="enabled">
                        <bool>false</bool>
                    </property>
                    <property name="text">
                        <string>Import/Export</string>
                    </property>
                    <property name="accel">
                        <string></string>
                    </property>
                </widget>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>btnConfig</cstring>
                    </property>
                    <property name="sizePolicy">
                        <sizepolicy>
                            <hsizetype>1</hsizetype>
                            <vsizetype>0</vsizetype>
                            <horstretch>0</horstretch>
                            <verstretch>0</verstretch>
                        </sizepolicy>
                    </property>
                    <property name="text">
                        <string>Co&amp;nfig</string>
                    </property>
                    <property name="accel">
                        <string>Alt+N</string>
                    </property>
                </widget>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>btnClose</cstring>
                    </property>
                    <property name="sizePolicy">
                        <sizepolicy>
                            <hsizetype>1</hsizetype>
                            <vsizetype>0</vsizetype>
                            <horstretch>1</horstretch>
                            <verstretch>0</verstretch>
                        </sizepolicy>
                    </property>
                    <property name="text">
                        <string>Cl&amp;ose</string>
                    </property>
                    <property name="accel">
                        <string>Alt+O</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Close this window</string>
                    </property>
                </widget>
            </hbox>
        </widget>
    </vbox>
</widget>
<tabstops>
    <tabstop>lneSearch</tabstop>
    <tabstop>lstHistory</tabstop>
    <tabstop>btnRestore</tabstop>
    <tabstop>btnDelete</tabstop>
    <tabstop>btnClose</tabstop>
</tabstops>
<layoutdefaults spacing="6" margin="11"/>
<includehints>
    <includehint>klineedit.h</includehint>
</includehints>
</UI>
